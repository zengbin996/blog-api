    "/user": {
      "post": {
        "tags": [
          "user"
        ],
        "summary": "注册",
        "operationId": "registerUser",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "username": {
                    "example": "admin1"
                  },
                  "password": {
                    "example": "Abcd1234!"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "example": {
                    "details": {
                      "acknowledged": true,
                      "insertedId": "651e7d340de9ad5ae24a6085"
                    },
                    "message": "success"
                  }
                }
              }
            }
          },
